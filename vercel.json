{
  "version": 2,
  "builds": [
    { "src": "api/server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/health", "dest": "/api/server.js" },
    { "src": "/mock-register", "dest": "/api/server.js" },
    { "src": "/webhook/(.*)", "dest": "/api/server.js" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
